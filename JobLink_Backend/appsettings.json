{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "${Issuer}",
    "Audience": "${Audience}",
    "SecretKey": "${SecretKey}",
    "AccessTokenExpiration": "${AccessTokenExpiration}",
    "RefreshTokenExpiration": "${RefreshTokenExpiration}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DefaultConnection}"
  }
}